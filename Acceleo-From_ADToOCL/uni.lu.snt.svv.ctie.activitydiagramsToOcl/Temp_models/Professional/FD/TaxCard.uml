        <ownedRule xmi:id="_cf0JEZOXEeS0vu9n5JWWng" name="TaxDeduction-CE-constraint" constrainedElement="_cf0JAJOXEeS0vu9n5JWWng">
          <specification xmi:type="uml:OpaqueExpression" xmi:id="_cf0JEpOXEeS0vu9n5JWWng">
            <language>OCL</language>
