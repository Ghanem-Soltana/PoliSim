        <ownedRule xmi:id="_xf7M_036EeSS5u8wdxTIGg" name="TaxDeduction-CE-constraint" constrainedElement="_xf7M7k36EeSS5u8wdxTIGg">
          <specification xmi:type="uml:OpaqueExpression" xmi:id="_xf7NAE36EeSS5u8wdxTIGg">
            <language>OCL</language>
