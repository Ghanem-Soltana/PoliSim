        <ownedRule xmi:id="_cw-z-JpAEeOXzvVK2MY5wA" name="TaxDeduction-CE-constraint" constrainedElement="_cw-z55pAEeOXzvVK2MY5wA">
          <specification xmi:type="uml:OpaqueExpression" xmi:id="_cw-z-ZpAEeOXzvVK2MY5wA">
            <language>OCL</language>
