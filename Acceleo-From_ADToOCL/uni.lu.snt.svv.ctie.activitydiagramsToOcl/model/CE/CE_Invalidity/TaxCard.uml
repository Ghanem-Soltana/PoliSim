          </specification>
        </ownedRule>
        <ownedRule xmi:id="_g4SCX5pAEeOXzvVK2MY5wA" name="ResidentTaxPayer-constraint" constrainedElement="_g4SCWppAEeOXzvVK2MY5wA">
