        <ownedRule xmi:id="_g4SCbZpAEeOXzvVK2MY5wA" name="TaxDeduction-CE-constraint" constrainedElement="_g4SCXJpAEeOXzvVK2MY5wA">
          <specification xmi:type="uml:OpaqueExpression" xmi:id="_g4SCbppAEeOXzvVK2MY5wA">
            <language>OCL</language>
