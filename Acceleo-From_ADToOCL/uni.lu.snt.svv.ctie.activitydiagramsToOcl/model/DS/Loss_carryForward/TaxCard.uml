        <ownedRule xmi:id="_sfxgKpo_EeOXzvVK2MY5wA" name="TaxDeduction-CE-constraint" constrainedElement="_sfxgGZo_EeOXzvVK2MY5wA">
          <specification xmi:type="uml:OpaqueExpression" xmi:id="_sfxgK5o_EeOXzvVK2MY5wA">
            <language>OCL</language>
