        <ownedRule xmi:id="_MuFM1po_EeOXzvVK2MY5wA" name="TaxDeduction-CE-constraint" constrainedElement="_MuFMxZo_EeOXzvVK2MY5wA">
          <specification xmi:type="uml:OpaqueExpression" xmi:id="_MuFM15o_EeOXzvVK2MY5wA">
            <language>OCL</language>
