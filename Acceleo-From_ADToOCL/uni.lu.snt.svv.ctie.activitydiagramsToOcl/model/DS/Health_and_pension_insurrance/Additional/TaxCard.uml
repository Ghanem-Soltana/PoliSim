        <ownedRule xmi:id="_CDcmAJpBEeOXzvVK2MY5wA" name="TaxDeduction-CE-constraint" constrainedElement="_CDcl75pBEeOXzvVK2MY5wA">
          <specification xmi:type="uml:OpaqueExpression" xmi:id="_CDcmAZpBEeOXzvVK2MY5wA">
            <language>OCL</language>
