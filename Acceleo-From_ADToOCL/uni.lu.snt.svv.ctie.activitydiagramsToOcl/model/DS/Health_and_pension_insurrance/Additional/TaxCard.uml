        <ownedRule xmi:id="_CDcl8JpBEeOXzvVK2MY5wA" name="NonResidentTaxPayer-constraint" constrainedElement="_CDclxJpBEeOXzvVK2MY5wA">
          <specification xmi:type="uml:OpaqueExpression" xmi:id="_CDcl8ZpBEeOXzvVK2MY5wA">
