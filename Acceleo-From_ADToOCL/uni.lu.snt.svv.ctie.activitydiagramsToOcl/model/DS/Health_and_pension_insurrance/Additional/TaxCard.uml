          </specification>
        </ownedRule>
        <ownedRule xmi:id="_CDcl8ppBEeOXzvVK2MY5wA" name="ResidentTaxPayer-constraint" constrainedElement="_CDcl7ZpBEeOXzvVK2MY5wA">
