          </specification>
        </ownedRule>
        <ownedRule xmi:id="_9yJLMJpAEeOXzvVK2MY5wA" name="ResidentTaxPayer-constraint" constrainedElement="_9yJLK5pAEeOXzvVK2MY5wA">
          <specification xmi:type="uml:OpaqueExpression" xmi:id="_9yJLMZpAEeOXzvVK2MY5wA">
