        <ownedRule xmi:id="_9yJvPppAEeOXzvVK2MY5wA" name="SubjectToWithholdingTax-constraint" constrainedElement="_9yJvPZpAEeOXzvVK2MY5wA">
          <specification xmi:type="uml:OpaqueExpression" xmi:id="_9yJvP5pAEeOXzvVK2MY5wA">
