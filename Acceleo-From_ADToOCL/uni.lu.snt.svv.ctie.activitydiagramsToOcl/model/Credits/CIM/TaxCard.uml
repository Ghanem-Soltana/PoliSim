        <ownedRule xmi:id="_Kvtn65pAEeOXzvVK2MY5wA" name="TaxDeduction-CE-constraint" constrainedElement="_Kvtn2ppAEeOXzvVK2MY5wA">
          <specification xmi:type="uml:OpaqueExpression" xmi:id="_Kvtn7JpAEeOXzvVK2MY5wA">
            <language>OCL</language>
