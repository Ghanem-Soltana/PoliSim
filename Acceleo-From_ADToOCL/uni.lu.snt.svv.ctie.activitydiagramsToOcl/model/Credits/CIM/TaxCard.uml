          </specification>
        </ownedRule>
        <ownedRule xmi:id="_Kvtn35pAEeOXzvVK2MY5wA" name="SubjectToIncomeTax-constraint" constrainedElement="_Kvtn2ppAEeOXzvVK2MY5wA">
